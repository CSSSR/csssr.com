{
  "name": "csssr.com",
  "scripts": {
    "dev": "nodemon --exec node $NODE_DEBUG_OPTION ./server/index.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node ./server/index.js",
    "lint:js": "eslint \"**/*.js\" --fix --max-warnings=0",
    "eslint-fix": "eslint --fix .",
    "analyze": "cross-env ANALYZE=1 next build",
    "optimize-images": "node -e 'require(\"optimize-png-for-web\")(process.argv[1], \"./optimized-images\", {})'"
  },
  "dependencies": {
    "@sentry/browser": "^4.4.2",
    "@sentry/node": "^4.3.4",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "babel-plugin-ramda": "^2.0.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "file-loader": "^3.0.1",
    "final-form": "^4.11.0",
    "final-form-focus": "^1.1.0",
    "i18next": "^11.9.0",
    "i18next-express-middleware": "^1.4.0",
    "i18next-node-fs-backend": "^2.1.0",
    "i18next-xhr-backend": "^1.5.1",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.1",
    "object-to-formdata": "^1.5.2",
    "optimize-png-for-web": "^0.1.5",
    "ramda": "^0.26.1",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-final-form": "^3.6.5",
    "react-gtm-components": "^0.5.0",
    "react-i18next": "^7.13.0",
    "react-transition-group": "2.5.3",
    "sitemap": "^2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "nodemon": "^1.18.8",
    "webpack-bundle-analyzer": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:js"
    }
  }
}
